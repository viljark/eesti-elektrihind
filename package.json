{
  "name": "eesti-elektrihind",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --dev-client",
    "prebuild": "npx expo prebuild --platform android --clean",
    "build:dev": "eas build --profile development --platform android",
    "build:android:preview": "yarn prebuild && eas build -p android --profile preview",
    "build:android:production": "yarn prebuild && eas build -p android --profile production",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production",
    "submit": "eas submit --platform android --profile production",
    "android": "npx expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "lint": "eslint **/*.{ts,tsx}"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo/config-plugins": "~6.0.0",
    "@gorhom/bottom-sheet": "^4.4.5",
    "@nandorojo/iconic": "^0.0.1",
    "@notifee/react-native": "^7.7.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-firebase/analytics": "^17.4.2",
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/crashlytics": "^17.4.2",
    "@react-native-firebase/perf": "^17.4.2",
    "@types/lodash": "^4.14.184",
    "eslint-plugin-jsx-expressions": "^1.3.1",
    "expo": "~48.0.18",
    "expo-background-fetch": "~11.1.1",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.2.1",
    "expo-haptics": "~12.2.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-notifications": "~0.18.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "lodash": "^4.17.21",
    "moti": "^0.24.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-svg": "13.4.0",
    "use-between": "^1.3.4",
    "victory-native": "^36.6.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.69.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "8.22.0",
    "eslint-plugin-react": "^7.31.8",
    "typescript": "^4.9.4"
  },
  "private": true,
  "resolutions": {
    "react-native-svg": "13.4.0"
  }
}
